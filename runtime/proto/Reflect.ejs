function Mixin(to, from) {
	if (!IsObject(to))
		throw new TypeError('Object expected');
	// Mixin with nil as the second arg is ignored.
	if (from === null || from === undefined)
		return to;
	if (!IsObject(from))
		throw new TypeError('Object expected');
	simile.mixin(to.Value, from.Value);
	return to;
