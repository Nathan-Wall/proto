function GetOwn(obj, key, receiver) {
	var K;
	if (obj === null || obj === undefined)
		return undefined;
	if (IsWrapper(key)) {
		if ('GetOwn' in key)
			return key.GetOwn(obj, receiver);
		else
			throw new Error(
				'Object cannot be used as a property key in get operation'
			);
	}
	K = ToString(key);
	if (hasOwn(obj.Value, K))
		return Get(obj, K, receiver);
	return undefined;
